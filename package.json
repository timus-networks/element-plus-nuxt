{
  "name": "@timus-networks/element-plus-nuxt",
  "version": "1.0.0",
  "description": "Element Plus module for Nuxt",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "@timus-networks/element-plus",
    "nuxt"
  ],
  "homepage": "https://github.com/timus-networks/element-plus-nuxt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timus-networks/element-plus-nuxt.git"
  },
  "bugs": {
    "url": "https://github.com/timus-networks/element-plus-nuxt/issues"
  },
  "scripts": {
    "build": "npm run dev:prepare && nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:start": "node playground/.output/server/index.mjs",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint . --fix --ext .ts,.vue,.js",
    "lint:test": "eslint . --ext .ts,.vue,.js --max-warnings 0",
    "prepack": "npm run build",
    "release": "npm run prepack && changelogen --release && npm publish && git push --follow-tags"
  },
  "peerDependencies": {
    "@element-plus/icons-vue": ">=0.2.6",
    "@timus-networks/element-plus": ">=1"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.3",
    "magic-string": "^0.27.0",
    "unplugin": "^1.11.0"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.12.3",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@timus-networks/element-plus": "^1.0.2",
    "@types/node": "^20.14.10",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxi": "^3.12.0",
    "nuxt": "^3.12.3",
    "typescript": "^5.5.3",
    "vue": "^3.4.31"
  }
}
